<div class="col-sm-9">
  <hr>
  <div class="col-sm-10"><h1><%= t "profile.history" %></h1>
  </div>
  <div class="card">
    <div class="card-body">
      <div class="table-responsive-sm">
        <table class="table table-striped">
          <thead>
            <tr>
              <th class="center"><%= t "profile.tbl_dau" %></th>
              <th ><%= t "profile.tbl_created" %></th>
              <th class="center"><%= t "profile.tbl_quantity" %></th>
              <th class="right"><%= t "profile.tbl_total" %></th>
              <th class="right"><%= t "profile.purchase_status" %></th>
              <th class="right"><%= t "profile.lbl_detail" %></th>
            </tr>
          </thead>
          <tbody>
            <div id="list-purchase" >
              <%= render(partial: "purchases/purchase", collection: @purchases) %>
            </div>
          </tbody>
        </table>
        <%= will_paginate @purchases %>
      </div>
    </div>
  </div>
</div>
